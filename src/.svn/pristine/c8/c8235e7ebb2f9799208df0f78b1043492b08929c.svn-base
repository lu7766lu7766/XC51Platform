class DmCenterWnd extends egret.DisplayObjectContainer{
    private _id;
    private _item:GHashMap<DmC_info>;

    constructor(id){
        super();

        this._id = id;
        this._item = new GHashMap<DmC_info>();
        //send协议

    }

    public upData(id):void{
        if(id == this._id && this.parent.parent!=undefined){
            let o = new DmC_infoData();
            let oo = new DmC_info_infoData();
            oo.id = 1;
            oo.team1 = "黄金海岸骑士";
            oo.team2 = "布里斯班前锋";
            oo.type = "澳足总";

            o.item.Gput(0,oo);
            o.item.Gput(1,oo);
            o.item.Gput(2,oo);

            o.id = 0;
            o.txName = "全网最红计划";
            o.money = "2000";
            o.content = "心水推荐计划，大家赶紧上车！心水推荐计划aaaaa";
            o.type = "竞彩篮球";
            o.multiple = "3.95";
            DmC_infoMsg.getInstance.item.Gput(0,o);
            DmC_infoMsg.getInstance.item.Gput(1,o);
            DmC_infoMsg.getInstance.item.Gput(2,o);
            DmC_infoMsg.getInstance.item.Gput(3,o);
            DmC_infoMsg.getInstance.item.Gput(4,o);
            DmC_infoMsg.getInstance.item.Gput(5,o);
            DmC_infoMsg.getInstance.item.Gput(6,o);
            DmC_infoMsg.getInstance.item.Gput(7,o);
            DmC_infoMsg.getInstance.item.Gput(8,o);

            let objHeight = 0;
            for(let key of DmC_infoMsg.getInstance.item.keys){
                let obj:DmC_info;
                if(this._item.GhasKey(key)){
                    obj = this._item.Gget(key);
                }else{
                    obj = new DmC_info();
                    this._item.Gput(key,obj);
                }
                obj.aa(DmC_infoMsg.getInstance.item.Gget(key));
                obj.y = objHeight;
                objHeight = objHeight + obj.height;
                if(obj.parent==undefined)
                    this.addChild(obj);
            }
            ToolMrg.upItemofGHashMap(this._item,DmC_infoMsg.getInstance.item);
        }

    }

}