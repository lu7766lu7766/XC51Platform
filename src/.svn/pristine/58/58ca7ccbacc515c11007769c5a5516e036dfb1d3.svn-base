class GoodsData {
	/**物品id */
	public gid:number;
	/**物品类型 */
	public type:number;

	/**商品标题 */
	public title:string;
	/**款式 */
	public style:string[] = [];
	/**性别属性 */
	public sex:string[] = [];
	/**颜色 */
	public color:string[] = [];
	/**尺码 */
	public size:string[] = [];
	/**商品描述 */
	public desc:string[] = [];
	/**商品价格 */
	public price:number;
	/**发货时间 */
	public deltime:string;
	/**库存数量 */
	public stock:number;
	/**商品缩略图 */
	public thumbImg:string[] = [];
	/**商品图片 */
	public bigImg:string[] = [];
	/**商品详情图片 */
	public detail:string[] = [];
	/**商品详情图片高度 */
	public detailHeight:number[] = [];
}