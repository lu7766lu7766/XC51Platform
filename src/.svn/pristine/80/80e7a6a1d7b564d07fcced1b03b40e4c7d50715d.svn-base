class MyLotteryData {
	/**id 标识*/
	public id:number;
	/**类型 1(竞足) 2(竞篮) 3(组三) 4(组5)*/
	public type:number;
	/**串关类型 1串关 2单关 */
	public clType:number;
	/**标题 */
	public title:string;
	/**下注金额 */
	public xzMoney:number;
	/**中奖金额 */
	public xjMoney:number;
	/**状态  1待开奖 2未中奖 3已中奖 */
	public statue:number;
	/**时间 */
	public time:number;
	
	public url:string;

	
}

/**篮球 足球 */
class FBLotData {
	/**周几 */
	public weekTime:string;
	/**对阵信息 主*/
	public aName:string;
	/**对阵信息 客*/
	public bName:string;
	/**过关方式 1(单关) 2(2串1) 3(3串1)...*/
	public passList:number[] = [];
	/**投注项 第一位下标id 第二位倍率*/
	public list:Array<number[]> = new Array<number[]>();
	/**赛果列表 */
	public fruitList:number[] = [];
}

