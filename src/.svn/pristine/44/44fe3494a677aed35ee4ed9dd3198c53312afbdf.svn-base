class GameValue {
	private static _mInstance: GameValue;
	public static get getInstance(): GameValue {
		if (GameValue._mInstance == undefined)
			GameValue._mInstance = new GameValue();
		return GameValue._mInstance;
	}
	private constructor() {
	}

